{
    "name": "tempest/icon",
    "description": "A component for working with icons.",
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
        "php": "^8.4",
        "tempest/container": "1.1.0",
        "tempest/http-client": "1.1.0",
        "tempest/support": "1.1.0",
        "symfony/cache": "^7.2"
    },
    "suggest": {
        "tempest/event-bus": "For events support"
    },
    "autoload": {
        "files": [
            "src/functions.php"
        ],
        "psr-4": {
            "Tempest\\Icon\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tempest\\Icon\\Tests\\": "tests"
        }
    }
}
